{
    "manifest_version": 3,
    "name": "Age Finder",
    "version": "1.0",
    "description": "Find approximate age based on education/work experience",
    "permissions": [
      "activeTab"
    ],
    "action": {
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png"
      },
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  